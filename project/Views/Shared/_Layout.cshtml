<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>@ViewData["Title"] - Proj_Interdisciplinar</title>
    
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="~/css/menu-responsivo.css" />
    <link rel="stylesheet" type="text/css" href="~/css/logo.css" />

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" integrity="sha512-Fo3rlrZj/k7ujTnHg4CGR2D7kSs0v4LLanw2qksYuRlEzO+tcaEPQogQ0KaoGN26/zrn20ImR1DfuLWnOo7aBA==" crossorigin="anonymous" referrerpolicy="no-referrer" />

    @RenderSection("styles", false)

    <script type="text/javascript" src=~/js/menu-responsivo.js />
    
    <script src="https://kit.fontawesome.com/64d58efce2.js"
            crossorigin="anonymous"></script>
</head>
<body>

    <header id="myNav" class="overlay">

        <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>


        <a href="/Home/Index"><h3>Pãozinhos da Vovó</h3></a>

        <div class="overlay-content">
            <a href="/Home/Index">Home</a>
            <a href="/Login/Login">Login</a>
            <a href="/Home/Index">Contatos</a>
        </div>


    </header>

    <button class="nav-button" onclick="openNav()">
        <i class="fa-solid fa-bars fa-2x"></i>
    </button>

    <div class="logo-one">
        <a href="/Home/Index"><img src="~/images/LogoOne.png" /></a>
    </div>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/compras.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
